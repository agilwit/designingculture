 <script>
 $("g.ringMenu").mouseover(function() { 
 document.querySelector("#wotwtb_animate_in").beginElement();
 document.querySelector("#videos_animate_in").beginElement();});
 $("g.ringMenu").mouseout(function() { 
 document.querySelector("#wotwtb_animate_out").beginElement();
 document.querySelector("#videos_animate_out").beginElement();});
 $("g.video_ray").click(function(){
 document.querySelector("#videos_move_to_title").beginElement();
 document.querySelector("#videos_animate_out").beginElement();});
 
  document.querySelector("#exhibit_animate_out").beginElement();});
 $("g.exhibit_ray").click(function(){
 document.querySelector("#exhibit_move_to_title").beginElement();
 document.querySelector("#exhibit_animate_out").beginElement();});
 
  document.querySelector("#walls_animate_out").beginElement();});
 $("g.walls_ray").click(function(){
 document.querySelector("#walls_move_to_title").beginElement();
 document.querySelector("#walls_animate_out").beginElement();});
 
  document.querySelector("#print_animate_out").beginElement();});
 $("g.print_ray").click(function(){
 document.querySelector("#print_move_to_title").beginElement();
 document.querySelector("#print_animate_out").beginElement();});
 
  document.querySelector("#dvd_animate_out").beginElement();});
 $("g.dvd_ray").click(function(){
 document.querySelector("#dvd_move_to_title").beginElement();
 document.querySelector("#dvd_animate_out").beginElement();});
 
  document.querySelector("#maps_animate_out").beginElement();});
 $("g.maps_ray").click(function(){
 document.querySelector("#maps_move_to_title").beginElement();
 document.querySelector("#maps_animate_out").beginElement();});
 
  document.querySelector("#blog_animate_out").beginElement();});
 $("g.blog_ray").click(function(){
 document.querySelector("#blog_move_to_title").beginElement();
 document.querySelector("#blog_animate_out").beginElement();});
 
 </script>
 
